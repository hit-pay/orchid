      "import": "./dist/CodeBlock.js"
    },
    "./TextEditor": {
      "import": "./dist/TextEditor.js"
    },
    "./Calendar": {
      "import": "./dist/Calendar.js"
    },
    "./Draggable": {
      "import": "./dist/Draggable.js"
    },
    "./StoreDesign": {
      "import": "./dist/StoreDesign.js"
    },
    "./ColorPicker": {
      "import": "./dist/ColorPicker.js"
    },
    "./EmojiPicker": {
      "import": "./dist/EmojiPicker.js"
