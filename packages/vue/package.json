{
  "name": "@orchidui/vue",
  "description": "Orchid UI , Dashboard Ui Library Vue 3 tailwind css",
  "version": "0.5.493",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "update-version": "npm version patch"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hit-pay/orchid.git",
